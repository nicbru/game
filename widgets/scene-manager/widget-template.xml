<kinetic
    name="{@name}-kinetic"
    position="{@position}"
    width="{@width}"
    height="{@height}"
    left="{@left}"
    right="{@right}"
    top="{@top}"
    bottom="{@bottom}"
    margin="{@margin}"
    margin-top="{@margin-top}"
    margin-bottom="{@margin-bottom}"
    margin-left="{@margin-left}"
    margin-right="{@margin-right}"
    padding="{@padding}"
    padding-top="{@padding-top}"
    padding-bottom="{@padding-bottom}"
    padding-left="{@padding-left}"
    padding-right="{@padding-right}"
    x-axis="{@x-axis}"
    y-axis="{@y-axis}"
    class="{@class}"
    overflow="{@overflow}">
    <content/>
</kinetic>

